# @since 1.6.0
type Bodgitlib::Netmask = Pattern[
  /(?x) ^ (?: 25[24] | 24[08] | 224 | 192 | 128 | 0 ) (?: \. 0 ){3} $/,
  /(?x) ^ 255 \. (?: 25[24] | 24[08] | 224 | 192 | 128 | 0 ) (?: \. 0 ){2} $/,
  /(?x) ^ (?: 255 \. ){2} (?: 25[24] | 24[08] | 224 | 192 | 128 | 0 ) \. 0 $/,
  /(?x) ^ (?: 255 \. ){3} (?: 255 | 25[24] | 24[08] | 224 | 192 | 128 | 0 ) $/,
]
